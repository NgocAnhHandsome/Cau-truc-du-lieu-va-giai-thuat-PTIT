#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;

int main() {
	int t; cin >> t;
	while (t--) {
		int a, b; cin >> a >> b;
		vector<int> m(a + b);
		for (int i = 0; i < a + b; i++)cin >> m[i];
		sort(m.begin(), m.end());
		for (int i = 0; i < a + b; i++)cout << m[i] << " ";
		cout << endl;
	}
}
