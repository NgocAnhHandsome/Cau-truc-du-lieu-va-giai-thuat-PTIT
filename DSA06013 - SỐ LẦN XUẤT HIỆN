#include<iostream>
#include<set>
#include<algorithm>
#include<vector>
using namespace std;
int main() {
	int t; cin >> t;
	while (t--) {
		int n, x; cin >> n >> x;
		int M[1000];
		for (int i = 0; i < n; i++)cin >> M[i];
		int cnt = 0;
		for (int i = 0; i < n; i++)
			if (M[i] == x)cnt++;
		if (cnt == 0)cout << -1 << endl;
		else
		cout << cnt << endl;
	}
}
