#include<iostream>
#include<set>
#include<algorithm>
#include<vector>
#include<climits>
using namespace std;
int main() {
	int t; cin >> t;
	while (t--) {
		int m, n; cin >> m >> n;
		int x;
		multiset<int> S;
		while (m--) {
			cin >> x; S.insert(x);
		}
		while (n--) {
			cin >> x; S.insert(x);
		}
		for (auto a : S)cout << a << " ";
		cout << endl;
	}
}
