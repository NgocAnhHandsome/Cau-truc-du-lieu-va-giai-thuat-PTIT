#include<iostream>
#include<iomanip>
using namespace std;
int test(long long int M[], long long int n) {
	for (int i = 0; i < 100; i++)
		if (M[i] == n)
			return 1;
	return 0;
}
int main() {

	
		int n; cin >> n;
		double x = 0;
		for (int i = 1; i <= n; i++)
			x += 1 * 1.0 / i;
		cout << setprecision(4) << fixed << x << endl;
	
}
