#include<iostream>
using namespace std;
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int m, n, a, b, d1 = 0, d2 = 0;
        cin >> m >> n >> a >> b;
        for (int i = m; i <= n; i++)
        {
            if (i % a == 0 || i % b == 0)
                d1++;
        }

        cout << d1 + d2 << endl;
    }
}
