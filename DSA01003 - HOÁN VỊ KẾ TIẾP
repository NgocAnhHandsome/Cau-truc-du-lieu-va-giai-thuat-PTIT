#include<iostream>
#include<string>
using namespace std;

int main() {
	int t; cin >> t;
	while (t--) {
		int n; cin >> n;
		int s[1000];
		for (int i = 1; i <= n; i++)cin >> s[i];
		int x = 0;
		for (int i = n - 1; i > 0; i--) {
			if (s[i] <= s[i + 1]) {
				x = 1;
				int k = n;
				while (s[k] < s[i])k--;
				swap(s[k], s[i]);
				int l = i + 1, r = n - 1;
				while (l < r) {
					swap(s[l], s[r]); l++; r--;
				}
				break;
			}
		}
		if (x == 0) {
			for (int i = 0; i < n; i++)cout << i + 1 << " ";
		}
		else
			for (int i = 1; i <= n; i++)cout << s[i] << " ";
		cout << endl;
	}
}
