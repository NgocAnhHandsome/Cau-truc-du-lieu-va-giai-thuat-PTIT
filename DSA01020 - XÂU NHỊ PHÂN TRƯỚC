#include<iostream>
#include<string>
using namespace std;
int main() {
	int t; cin >> t;
	while (t--) {
		string s; cin >> s;
		int x = 0;
		for (int i = s.length() - 1; i >= 0; i--) {
			if (s[i] == '1') {
				s[i] = '0';
				x = 1;
				for (int j = i + 1; j < s.length(); j++) {
					s[j] = '1';
				}
				break;
			}
		}
		if (x == 0) {
			for (int i = 0; i < s.length(); i++)cout << 1;
		}
		else cout << s;
		cout << endl;
	}
}
