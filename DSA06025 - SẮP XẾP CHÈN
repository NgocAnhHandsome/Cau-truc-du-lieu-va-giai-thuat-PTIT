#include<iostream>
#include<cmath>
#include<algorithm>
#include<vector>
using namespace std;
int main() {
	int n; cin >> n;
	int M[1000];
	for (int i = 0; i < n; i++)cin >> M[i];
	for (int i = 1; i <= n; i++) {
		sort(M, M + i);
		cout << "Buoc " << i-1 << ": ";
		for (int j = 0; j < i; j++)cout << M[j] << " ";
		cout << endl;
	}
	
}
